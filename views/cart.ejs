<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/nav.css">
    <link rel="stylesheet" href="/css/cart.css">
    <title>Cart</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Playwrite+HU:wght@100..400&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap" rel="stylesheet">
    <link
        href="https://cdn.jsdelivr.net/npm/remixicon@4.3.0/fonts/remixicon.css"
        rel="stylesheet"
    />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <link rel="shortcut icon" href="/img/logo-with.png" type="image/x-icon">
  <!-- locomotive css cdn  -->
  <link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/locomotive-scroll@3.5.4/dist/locomotive-scroll.css"
/>
<link href=
"https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css"
		rel="stylesheet">
	<script src=
"https://code.jquery.com/jquery-1.10.2.js">
	</script>
	<script src=
"https://code.jquery.com/ui/1.10.4/jquery-ui.js">
	</script>

</head>
<body>
    <div id="main">
        <nav>
            <div class="l">
              <a href="/home"><img src="/img/logowithout.png" alt=""></a>
            </div>
            <div class="m">
              <a href="/address">Add Address</a>
              <a  href="/home">Continue Shopping</a>
            </div>
            <div class="r">
              <a href="/cart"><i class="ri-shopping-cart-2-line"></i></a>
    
              <h3><%= user.username  %></h3>
              <div class="div">
                <a class="" href="/profile"><img src="/img/upload/<%= user.profilepic  %>" alt=""></a>
              </div>
              <a class="logout" href="/logout">Logout</a>
            </div>
    
          </nav>
          
          
          <div class="page">
              <% cart.forEach(function(c){ %>
            <div class=" ss" >
              <form action="/proceed/<%= c._id  %>" method="post">

                <div class="d">
                  
                  <div class=" dl">
                    <img src="/img/upload/<%= c.postid.picture %>" class="img-fluid rounded-start" alt="">
                  </div>
                  <div class="dr">
                      <h5 class=""><%= c.postid.title  %></h5>
                      <select name="qulity" id="">
                        <!-- <option value="00">0</option> -->
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>

                      </select>
                      <p class=""><small class="text-body-secondary">&#8377 <%= c.postid.price.toLocaleString("en-IN")  %></small></p>
                      
                      
                        <select class="ass " name="address" id="">
                          <option  value="select">Address</option>
                      <% address.forEach(function(a){ %>
                          <option value="
                            <%=a.fullName%> 
                            <%=a.phoneNumber%> 
                            <%=a.addressLine1%> 
                            <%=a.addressLine2%>
                          ">
                            Address 1
                          </option>
                          <% }) %>
                        </select>
                          <!-- <label for="startDate">Start Date:</label>
                          <input type="text" class="form-control" id="date1" name="startDate" required>
                          <label for="endDate">End Date:</label>
                          <input type="text" class="form-control" id="date2" name="endDate" required> -->
                          <p>Start Date</p>
                          <input type="text" name="startDate" id="date1">
                        <p>End Date</p>
                        <input type="text"  name="endDate" id="date2">
                          
                          <input type="submit" 
                          onclick="func()" class="pro" id=" sub" value="Proceed">
                          <a href="/cart/delete/<%=c._id%>">Remove</a>
                  </div>
                </div>
              </form>

            </div>
            <%  }) %>

          </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
   <!--locomotive js --> 
   <script src="https://cdn.jsdelivr.net/npm/locomotive-scroll@3.5.4/dist/locomotive-scroll.js"></script>
   <!--gsap  --> 
   <script
     src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js"
     integrity="sha512-7eHRwcbYkK4d9g/6tD/mhkf++eoTHwpNM9woBxtPUBWm67zeAfFC+HrdoE2GanKeocly/VxeLvIqwvCdk7qScg=="
     crossorigin="anonymous"
     referrerpolicy="no-referrer"
   ></script>
    <!--Scroll tigger  --> 
   <script
     src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/ScrollTrigger.min.js"
     integrity="sha512-onMTRKJBKz8M1TnqqDuGBlowlH0ohFzMXYRNebz+yOcc5TQr/zAKsthzhuv0hiyUKEiQEQXEynnXCvNTOk50dg=="
     crossorigin="anonymous"
     referrerpolicy="no-referrer"
   ></script>
   <script src="/js/item.js"></script>
   <script>
  

  $(function() {
			$("#date1").datepicker();
			var date1 = $("#date1")
		});
		$(function() {
			$("#date2").datepicker();
			var date2 = $("#date2")
		});
	
		function func() {
			date1 = new Date(date1.value);
			date2 = new Date(date2.value);
			var milli_secs = date1.getTime() - date2.getTime();
			
			// Convert the milli seconds to Days 
			var days = milli_secs / (1000 * 3600 * 24);
			document.getElementById("ans").value = 
			Math.round(Math.abs(days));
		}
</script>
   
</body>
</html>